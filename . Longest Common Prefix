class Solution {
public:
    string longestCommonPrefix(vector<string>& strs) {
       for (int i = 0; i < words[0].size(); i++) {
        char c = words[0][i];

     
        for (int j = 1; j < words.size(); j++) {
           
            if (i >= words[j].size() || words[j][i] != c) {
                return prefix;
            }
        } 
    }
};
